{
    "name": "muku9812/nepalidate",
    "description": "Laravel package to convert AD to BS that can work with carbon (forked from pratiksh/nepalidate)",
    "keywords": [
        "muku9812",
        "nepalidate",
        "laravel",
        "carbon",
        "nepali-date"
    ],
    "homepage": "https://github.com/muku9812/nepalidate",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mukesh",
            "email": "mukesh@yajtechnologies.com",
            "role": "Developer"
        },
        {
            "name": "Pratik Shrestha",
            "email": "pratikdai404@gmail.com",
            "role": "Original Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Muku9812\\Nepalidate\\": "src"
        },
        "files": [
            "src/Helpers/NepaliDateHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Muku9812\\Nepalidate\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Muku9812\\Nepalidate\\Providers\\NepalidateServiceProvider"
            ],
            "aliases": {
                "Nepalidate": "Muku9812\\Nepalidate\\Facades\\NepaliDate"
            }
        }
    }
}